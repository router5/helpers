  "bugs": {
    "url": "https://github.com/router5/helpers/issues"
  },
  "homepage": "https://router5.github.io",
  "devDependencies": {
    "babel-core": "~6.9.1",
    "babel-eslint": "~6.0.4",
    "babel-plugin-transform-es2015-modules-umd": "~6.12.0",
    "babel-preset-es2015": "^6.16.0",
    "eslint": "~2.11.1",
    "mocha": "~2.5.3",
    "rimraf": "~2.5.2",
    "should": "~1.17.6"
  }
}
